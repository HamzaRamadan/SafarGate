rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // This rule allows any authenticated user to read and write to any document in the database.
    // It is written to correctly handle both document `get` and collection `list` operations.
    
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
